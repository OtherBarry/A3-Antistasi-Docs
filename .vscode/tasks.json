{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Generate LOCAL HTML",
            "command": "${workspaceFolder}/.venv/Scripts/antistasi-sqf-tools.exe",
            "args": ["docs", "build", "-b", "html"],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": true
            }
        }
    ]
}
